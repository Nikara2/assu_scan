# Configuration de la base de données PostgreSQL
# Railway fournit automatiquement cette URL quand vous ajoutez PostgreSQL
DATABASE_URL="postgresql://user:password@host:5432/database?schema=public"

# Port du serveur (Railway définit automatiquement PORT)
PORT=3001

# URL du frontend pour CORS (votre URL Vercel)
FRONTEND_URL="https://votre-frontend.vercel.app"
